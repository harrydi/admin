<?xml version="1.0"?>
<config>
  <modules>
    <Omniesolutions_General>
      <version>0.1.1</version>
    </Omniesolutions_General>
  </modules>
  <frontend>
    <routers>
      <general>
        <use>standard</use>
          <args>
            <module>Omniesolutions_General</module>
            <frontName>retailer-inquery</frontName>
          </args>
      </general>
    </routers>
		<layout>
		  <updates>
			<general>
			  <file>general.xml</file>
			</general>
		  </updates>
		</layout>
  </frontend>
  <global>
    <helpers>
      <general>
        <class>Omniesolutions_General_Helper</class>
      </general>
    </helpers>
	<blocks>
	  <general>
		<class>Omniesolutions_General_Block</class>
	  </general>
	</blocks>
	<models>
	  <general>
		<class>Omniesolutions_General_Model</class>
		<resourceModel>general_mysql4</resourceModel>
	  </general>
	  <general_mysql4>
		<class>Omniesolutions_General_Model_Mysql4</class>
		<entities>		  
			  <general>
				<table>general</table>
			  </general>
        </entities>
	  </general_mysql4>
	</models>
	<resources>
	  <general_setup>
		<setup>
		  <module>Omniesolutions_General</module>
		</setup>
		<connection>
		  <use>core_setup</use>
		</connection>
	  </general_setup>
	  <general_write>
		<connection>
		  <use>core_write</use>
		</connection>
	  </general_write>
	  <general_read>
		<connection>
		  <use>core_read</use>
		</connection>
	  </general_read>
	</resources>
  </global>
  <admin>
	<routers>
	  <general>
	    <use>admin</use>
		<args>
		  <module>Omniesolutions_General</module>
		  <frontName>admin_general</frontName>
		</args>
	  </general>
	</routers>
  </admin>
  <adminhtml>
	<menu>
	  <general module="general">
		<title>General</title>
		<sort_order>100</sort_order>
		<children>
		  <general module="general">
		    <title>Manage General</title>
			<sort_order>0</sort_order>
			<action>admin_general/adminhtml_general</action>
		  </general>
		</children>
	  </general>
	</menu>
	<acl>
	  <resources>
		<all>
		  <title>Allow Everything</title>
		</all>
		<admin>
		  <children>
			<general translate="title" module="general">
			  <title>General</title>
			  <sort_order>1000</sort_order>
			  <children>
		  <general translate="title">
			<title>Manage General</title>
			<sort_order>0</sort_order>
		  </general>
			  </children>
			</general>
		  </children>
		</admin>
	  </resources>
	</acl>
	<layout>
	  <updates>
		<general>
		  <file>general.xml</file>
		</general>
	  </updates>
	</layout>
  </adminhtml>
</config> 